
(rules PCB Z80Mini
  (snap_angle 
    fortyfive_degree
  )
  (autoroute_settings
    (fanout off)
    (eu.mihosoft.freerouting.autoroute on)
    (postroute on)
    (vias on)
    (via_costs 50)
    (plane_via_costs 5)
    (start_ripup_costs 100)
    (start_pass_no 9900)
    (layer_rule F.Cu
      (active on)
      (preferred_direction horizontal)
      (preferred_direction_trace_costs 1.0)
      (against_preferred_direction_trace_costs 2.8)
    )
    (layer_rule B.Cu
      (active on)
      (preferred_direction vertical)
      (preferred_direction_trace_costs 1.0)
      (against_preferred_direction_trace_costs 1.5)
    )
  )
  (rule
    (width 250.0)
    (clear 200.2)
    (clear 125.0 (type smd_to_turn_gap))
    (clear 50.0 (type smd_smd))
  )
  (padstack "Via[0-1]_800:400_um"
    (shape
      (circle F.Cu 800.0 0.0 0.0)
    )
    (shape
      (circle B.Cu 800.0 0.0 0.0)
    )
    (attach off)
  )
  (via 
    "Via[0-1]_800:400_um" "Via[0-1]_800:400_um" default
  )
  (via 
    "Via[0-1]_800:400_um-kicad_default" "Via[0-1]_800:400_um" "kicad_default"
  )
  (via_rule
    default "Via[0-1]_800:400_um"
  )
  (via_rule
    "kicad_default" "Via[0-1]_800:400_um-kicad_default"
  )
  (class default
    (clearance_class default)
    (via_rule default)
    (rule
      (width 250.0)
    )
    (circuit 
      (use_layer F.Cu B.Cu)
    )
  )
  (class "kicad_default"
    GND +5V "Net-(C6-Pad1)" "Net-(C10-Pad1)" "Net-(C11-Pad2)" "Net-(C12-Pad2)" "Net-(C13-Pad2)" "Net-(C14-Pad1)"
    "Net-(C15-Pad1)" NMI WAIT RD /D0 /D2 /D4 /D6
    WR /A1 /A3 /A5 /A7 /IORQ /A9 /A11
    /BUSACK /A14 /A15 /A13 /BUSRQ /A12 /A10 /A8
    /A6 /A4 /A2 /A0 /D7 /D5 /D3 /D1
    /M1 /RESET HALT "Net-(R3-Pad1)" MREQ /INT "Net-(U1-Pad28)" "Net-(U1-Pad6)"
    "Net-(U2-Pad20)" "Net-(U3-Pad26)" "Net-(U3-Pad20)" "Net-(U3-Pad1)" "Net-(U4-Pad13)" "Net-(U4-Pad2)" "IO_RESET" "Net-(U8-Pad37)"
    "Net-(U8-Pad17)" "Net-(U8-Pad36)" "Net-(U8-Pad16)" "Net-(U8-Pad15)" "Net-(U8-Pad34)" "Net-(U8-Pad33)" "Net-(U8-Pad32)" "Net-(U8-Pad31)"
    "Net-(U8-Pad11)" "Net-(U8-Pad10)" "Net-(U8-Pad29)" "Net-(U8-Pad24)" "Net-(U8-Pad23)" "Net-(U9-Pad28)" "Net-(U9-Pad27)" "Net-(U9-Pad23)"
    "Net-(U9-Pad22)" "Net-(U9-Pad19)" "Net-(U9-Pad14)" "Net-(U9-Pad13)" "Net-(U9-Pad12)" "Net-(U9-Pad6)" "Net-(X1-Pad1)" "Net-(J3-Pad4)"
    "Net-(D1-Pad2)" "Net-(D2-Pad2)" "Net-(R8-Pad2)" "Net-(R9-Pad2)" "Net-(D3-Pad2)" "Net-(X2-Pad1)" "Net-(R11-Pad1)" "Net-(R12-Pad1)"
    "Net-(R13-Pad1)" "Net-(R14-Pad1)" "/IO_UART/PA4" "/IO_UART/PA5" "/IO_UART/PB1" "/IO_UART/PA6" "/IO_UART/PB0" "/IO_UART/PA7"
    "/IO_UART/PC3" "/IO_UART/PC2" "/IO_UART/PC1" "/IO_UART/PC0" "/IO_UART/PC4" "/IO_UART/PC5" "/IO_UART/PC6" "/IO_UART/PC7"
    "/IO_UART/PB7" "/IO_UART/PB6" "/IO_UART/PB5" "/IO_UART/PB4" "/IO_UART/IO_CE" "/IO_UART/UART_CE" "/IO_UART/UART_INTR"
    (clearance_class "kicad_default")
    (via_rule kicad_default)
    (rule
      (width 250.0)
    )
    (circuit 
      (use_layer F.Cu B.Cu)
    )
  )
)